        "target": "ESNext",
        "sourceMap": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noEmit": true,
        "declaration": false,
        "module": "esnext",
        "resolveJsonModule": true,
        "skipLibCheck": true,
    },
    "include": [
        "**/*.ts",
    ]
}